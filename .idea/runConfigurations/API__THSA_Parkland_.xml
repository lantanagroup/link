<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="API (THSA-Parkland)" type="SpringBootApplicationConfigurationType" factoryName="Spring Boot">
    <module name="api" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.lantanagroup.link.api.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <option name="SPRING_BOOT_MAIN_CLASS" value="com.lantanagroup.link.api.ApiApplication" />
    <option name="VM_PARAMETERS" value="-Dserver.port=8082 -Dapi.skip-init=true" />
    <option name="ALTERNATIVE_JRE_PATH" />
    <option name="SHORTEN_COMMAND_LINE" value="NONE" />
    <envs>
      <env name="SPRING_CONFIG_LOCATION" value="file:/${USERPROFILE}\Lantana Consulting Group\Link - Configs\api-parkland.yml" />
    </envs>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>